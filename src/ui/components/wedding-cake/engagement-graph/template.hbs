<div class="dashboard__sub-numbers">
    <div class="dashboard__sub-numbers--graph">
      <svg class="chart">
          <g>
              <rect
                  class="background"
                  width="100%"
                  height="100%"
              ></rect>

              <rect
                  class="bar"
                  height="100%"
                  width="{{weightedEngagementNumber}}%"
              ></rect>
          </g>

            <g>
                {{#each weightedMilestones key="@index" as |milestone|}}
                    <line
                        style="stroke: green; stroke-width: 1"
                        x1="{{milestone}}%"
                        x2="{{milestone}}%"
                        y1="0"
                        y2="100%"
                    ></line>
                {{/each}}
            </g>
      </svg>
    </div>
  </div>
